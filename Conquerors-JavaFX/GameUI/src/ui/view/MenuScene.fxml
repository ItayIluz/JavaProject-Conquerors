<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox alignment="TOP_CENTER" minHeight="768.0" minWidth="1024.0" prefHeight="768.0" prefWidth="1024.0" spacing="15.0">
         <children>
            <Label text="Conquerors">
               <font>
                  <Font size="50.0" />
               </font>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Label>
            <Separator prefWidth="1024.0">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Separator>
            <Label fx:id="gameDataLoadedLabel" text="No Game Data Loaded" />
            <ProgressBar fx:id="gameDataLoadedProgressBar" prefWidth="200.0" progress="0.0" />
            <Separator prefHeight="37.0" prefWidth="1024.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Separator>
            <Button minWidth="120.0" mnemonicParsing="false" onAction="#handleLoadXML" prefWidth="120.0" text="Load XML File">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleLoadGame" text="Load Game">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleQuit" text="Quit">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </content>
</ScrollPane>
